# Write your MySQL query statement below
select MAX(x.Salary) as SecondHighestSalary from (Select emp.Salary from Employee emp order by 1 desc) x limit 2 offset 1;

select distinct salary from Employee order by 1 desc limit 1 offset 1

SELECT DISTINCT
    Salary AS SecondHighestSalary
FROM
    Employee
ORDER BY Salary DESC
LIMIT 1 OFFSET 1
