class Solution {
    public List<String> fizzBuzz(int n) {
        List<String> l=new ArrayList<String>();
        for(int i=1;i<n+1;i++){
            if(i%5==0){
                if(i%15==0){
                l.add("FizzBuzz");
                }
                else{
                    l.add("Buzz");
                }
            }
            else if(i%3==0){
                l.add("Fizz");
            }
            else{
                l.add(String.valueOf(i));
            }
        }
        return l;
    }
}
